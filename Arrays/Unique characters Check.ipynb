    "def uni_characters(s):\n",
    "    \n",
    "    chars = set()\n",
    "    \n",
    "    for c in s:\n",
    "        if c in chars:\n",
    "            return False\n",
    "        else:\n",
    "            chars.add(c)\n",
    "    return True\n",
    "\n",
    "uni_characters('sfsda')"
   ]
